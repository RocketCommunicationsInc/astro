{
  "name": "@astrouxds/react",
  "sideEffects": false,
  "version": "0.0.6",
  "description": "Astro Web Components React Wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/RocketCommunicationsInc/astro-in-react"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c",
    "test": "jest",
    "test.watch": "jest --watchAll"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.12.2",
    "@types/react": "^16.14.15",
    "@types/react-dom": "^16.7.0",
    "jest": "^27.2.1",
    "jest-dom": "^3.0.2",
    "np": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@astrouxds/astro-web-components": "0.0.16",
    "ts-jest": "^27.0.5"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist",
      "common"
    ]
  }
}
